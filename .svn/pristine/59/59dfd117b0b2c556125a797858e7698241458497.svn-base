/* header */
.header .top-bar {
    padding: 3px 0;
    background: #ffffff;
    color: #fff;
    height: 28px;
}
.header .top-bar .carrers-icon {
    margin-left: 23%;
    position: relative;
    z-index: 10;
}
.header .top-bar .carrers-icon  .icon {
    display: inline-block;
    margin: 0 10px;
    vertical-align: top;
}
.header .top-bar .carrers-icon  .icon:not(.icon-vinaphone){
    box-shadow: 0 0 10px 0 rgba(0,0,0,0.3);
    padding: 4px 7px;
    width: 80px;
    height: 40px;
    border-radius: 7px;
    text-align: center;
}
.header .top-bar .carrers-icon  .icon img {
    max-width: 100%;
}
.header .top-bar .carrers-icon  .icon:nth-child(3){
    padding-top: 7px;
}
.header .top-bar .carrers-icon  .icon:nth-child(4){
    padding-top: 8px;
}
.header .top-bar .carrers-icon .icon-vinaphone {
    background: #00a1e5;
    padding: 10px 10px 12px 10px;
    border-radius: 7px 7px 7px 7px;
    box-shadow: 0 0 10px 0 rgba(0,0,0,0.3);
}
.header .top-bar .carrers-icon .active {
    background: #00a1e5;
    height: 50px !important;
    padding: 13px 10px 17px 10px !important;
    border-radius: 7px 7px 0 0 !important;
    box-shadow: none !important;
}
.header .top-bar form.form-group .form-control {
    box-shadow: none;
    border-color: #f0f0f0;
}
.header .top-bar form.form-group {
    padding-right: 5px;
    position: relative;
    z-index: 4;
}
.header .top-bar form.form-group .form-control {
    /*    width: 250px;*/
    max-width: 100%;
}
.header .top-bar form.form-group .form-control:hover,
.header .top-bar form.form-group .form-control:focus,
.header .top-bar form.form-group .form-control:active{
    outline: none;
    border-color: #f3f3f3;
}
.navigate {
    background: #00a1e5;
    padding: 5px 0 0;
    position: relative;
    font-size: 0;
}
.navigate .over {
    position: absolute;
    background: #00a1e5;
    height: 25px;
    width: 332px;
    top: -25px;
    left: 0;
    overflow: hidden;
}
.navigate .over:after {
    content: '';
    background: #ffffff;
    height: 25px;
    width: 50px;
    position: absolute;
    top: -10px;
    right: -20px;
    z-index: 3;
    transform: rotate(45deg);
}
.navigate .logo {
    display: inline-block;
    vertical-align: top;
    width: 180px;
    padding-bottom: 10px;
}
.navigate .logo img {
    max-width: 100%;
}
.navigate ul {
    display: inline-block;
    vertical-align: top;
    list-style: none;
    padding-left: 50px;
    margin-bottom: 0;
    font-size: 14px;
    /*max-width: calc(100% - 180px);*/
    /*display: none;*/
}
.navigate ul.active{
    display: inline-block !important;
}
.navigate  ul > li {
    display: inline-block;
    vertical-align: top;
}
.navigate  ul > li > a {
    display: block;
    padding: 25px 5px 22px;
    margin: auto 10px;
    color: #ffffff;
    text-transform: uppercase;
    font-size: 15px;
    border-bottom: 3px solid transparent;
    transition: all .3s;
}
.navigate  ul > li > a:hover{
    text-decoration: none;
    color: #ffffff;
    border-bottom-color: #ffffff;
}
.navigate  ul > li.active > a {
    color: #ffffff;
    border-bottom-color: #ffffff;
}
