    .navigate .menu {
        display: inline-block;
        vertical-align: top;
        list-style: none;
        padding-left: 50px;
        margin-bottom: 0;
        font-size: 14px;
        float: left;
        /*max-width: calc(100% - 180px);*/
        /*display: none;*/
    }
    .navigate .menu.active{
        display: inline-block !important;
    }
    .navigate  .menu > li {
        display: inline-block;
        vertical-align: top;
        position: relative;
    }
    .navigate  .menu > li > a {
        display: block;
        padding: 25px 5px 22px;
        margin: auto 10px;
        color: #ffffff;
        text-transform: uppercase;
        font-size: 17px;
        border-bottom: 3px solid transparent;
        transition: all .3s;
    }
    .navigate .menu > li .sub-menu{
        display: none;
        position: absolute;
        top: 65px;
        left: 0px;
        background-color: #00a1e5;
        width: 300px;
        padding-left: 0px;
        z-index: 1000;
    }
    .navigate .menu > li .sub-menu li{
        clear: both;
        width: 100%;
        border-bottom: 1px solid #fff;
    }
    .navigate .menu > li .sub-menu li a{
        padding: 10px 0px;
        border-bottom: none;
        z-index: 1000;
    }
    .navigate .menu > li > a:hover{
        text-decoration: none;
        color: #ffffff;
    }
    .navigate .menu > li:hover .sub-menu{
        display: block;
    }
    .navigate  .menu > li.active > a {
        color: #ffffff;
        border-bottom-color: #ffffff;
    }
}
