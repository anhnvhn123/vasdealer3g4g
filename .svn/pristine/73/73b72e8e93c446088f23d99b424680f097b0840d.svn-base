.owl-dots {
    margin-top: 0px !important;
}

.entry-content img {
    max-width: 100%;
    height: auto;
}

.entry-content table {
    width: 100% !important;

}

.entry-content p {
    margin-bottom: 10px;
}

.entry-content td, .entry-content th {
    padding: 5px;
}

.wraper {
    width: 100%;
    max-width: 420px;
    margin: auto;
}

.wrap-header {
    padding-top: 7px;
}

.wrap-header .tab a {
    height: 41px;
    display: inline-block;
    padding: 0px 10px;
    border-top-right-radius: 6px;
    border-top-left-radius: 6px;
    width: 92px;
    text-align: center;
    border: 1px solid #ccc;
    border-bottom: none;
    float: left;
    margin-left: 2px;
}

.wrap-header .tab a.active {
    background-color: #0095f8;
    border: 1px solid #0095f8;
}

.wrap-header .tab a img {
    vertical-align: middle;
    line-height: 40px;
    padding-top: 13px;
    display: block;
}

.logo {
    margin-top: 10px;
    margin-left: 9px;
}

.menu {
    background-color: #0095f8;
    height: 48px;
}

.menu a {
    display: block;
    float: left;
    line-height: 48px;
    color: #fff;
    text-transform: uppercase;
    position: relative;
}

.menu a img {
    position: absolute;
    bottom: -11px;
    left: 40%;
    display: none;
    z-index: 999;
}

.menu a.current img {
    display: block;
}

.section1 {
    position: relative;
}

.btn-dangky {
    display: block;
    width: 150px;
    height: 40px;
    border-radius: 5px;
    margin: auto;
    text-align: center;
    line-height: 40px;
    background-color: #ff29b0;
    color: #fff;
    text-transform: uppercase;
    font-size: 16px;
}

footer {
    background-color: #0095f8;
    overflow: hidden;
    width: 100%;
    height: 55px;
    position: fixed;
    bottom: 0px;
    left: 0px;
    z-index: 9999;
}

footer a {
    width: 33%;
    float: left;
    height: 100%;
    display: block;
    text-align: center;
    border-right: 1px solid #fff;
}

footer a:last-child {
    border-right: none;
}

footer a img {
    line-height: 53px;
    vertical-align: bottom;
    display: block;
    margin: auto;
    margin-top: 17px;
}

.post-item {
    padding-bottom: 15px;
    margin-bottom: 15px;
    border-bottom: 1px solid #dfdfdf;
    overflow: hidden;
}

.goicuoc-item {
    height: 90px;
    background-color: #56bbff;
    border-radius: 2px;
    line-height: 90px;
    padding: 0px 20px;
    margin-bottom: 10px;
}

.goicuoc-item p {
    line-height: 1.5;
    display: inline-block;
    vertical-align: middle;
    /*font-size: 20px;*/
    font-size: 14px;
    color: #fff;
    text-transform: uppercase;
    text-align: center;
    width: 100%;
    font-weight: 600;
}

.goicuocd1-item {
    background-color: #f1f7fb;
    overflow: hidden;
    padding: 10px;
    margin-bottom: 14px;
}

.table-goicuocd2 {
    border: none;
}

.table-goicuocd2 tr {
    border-bottom: 2px solid #fff;
}

.table-goicuocd2 tr td:nth-child(2) {
    border-left: 1px solid #fff;
}

.table-goicuocd2 td {
    background-color: #1a8ddb;
    padding: 7px 10px;
    color: #fff;
}

.tab .logo-icon img.active {
    display: none;
}

.logo-icon a img.unactive {
    display: block;
}

.logo-icon.active img.active {
    display: block;
}

.logo-icon.active img.unactive {
    display: none;
}

.bread a {
    color: #ff29b0;
    text-transform: uppercase;
}

.bread span {
    text-transform: uppercase;
}

.logo-icon.viettel.active {
    background-color: #00918c !important;
    border: 1px solid #00918c !important;
}

.logo-icon.mobifone.active {
    background-color: #00579c !important;
    border: 1px solid #00579c !important;
}

.pagination {
    text-align: center;
    width: 100%;
}

.pagination a, .pagination span {
    display: inline-block;
    border: 1px solid #0095f8;
    padding: 3px 11px;
    color: #666;
}
.details-package .btn-dangky{
    margin-left: 15px;
    margin-right: 15px;
}
.pagination span.current, .pagination .next, .pagination .prev {
    border: #0095f8;
    background-color: #0095f8;
    color: #fff;
    padding: 4px 11px;
}

@media screen and (width: 320px) {
    .wrap-header .tab a {
        width: 83px !important;
    }
    #tawto-iframe-modal .modal-dialog {
        height: 350px;
        width: 90%;
        margin: auto;
        margin-top: 20px;
    }
    .modal-index .modal-dialog{
        position: absolute;
        bottom: 50px;
        height: 50px;
        background-image: url(../mobile/images/guide_chat.png);
        background-size: 100% 100%;
        width: 60%;
        right: 10%;
    }
}